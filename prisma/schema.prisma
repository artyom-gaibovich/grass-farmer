generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["grass"]
}

model GrassUser {
  id          String    @id
  proxies Json @db.JsonB
  created_at  DateTime? @default(now())
  updated_at  DateTime?

  @@map("grass_users")
  @@schema("grass")
}

